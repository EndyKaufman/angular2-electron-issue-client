{
    "name": "angular2-electron-issue-client",
    "version": "0.0.1",
    "description": "Simple client for issue trackers (redmine, jira, asana, bitbucket, github)",
    "scripts": {
        "start": "npm run clear && webpack-dev-server --config config/webpack.dev.js --port 3000 --inline --progress",
        "start:electron": "npm start --prefix ./electron/dist",
        "build": "npm run clear && rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
        "build:gh-pages": "npm run clear && rimraf gh-pages && webpack --config config/webpack.gh-pages.js --progress --profile --bail",
        "build:electron": "npm run clear && rimraf electron/dist && cp -fR electron/src electron/dist && webpack --config config/webpack.electron.js --progress --profile --bail",
        "build:all": "npm run build && npm run build:gh-pages && npm run build:electron",        
        "lite": "lite-server -c config/bs-server.json",
        "clear": "node config/clear.src.js",
        "postinstall": "typings install && npm install --prefix ./electron/src"
    },
    "license": "MIT",
    "dependencies": {
        "@angular/common": "2.0.0-rc.1",
        "@angular/compiler": "2.0.0-rc.1",
        "@angular/core": "2.0.0-rc.1",
        "@angular/http": "2.0.0-rc.1",
        "@angular/platform-browser": "2.0.0-rc.1",
        "@angular/platform-browser-dynamic": "2.0.0-rc.1",
        "@angular/router": "2.0.0-rc.1",
        "@angular/router-deprecated": "2.0.0-rc.1",
        "@angular/upgrade": "2.0.0-rc.1",
        "angular2-in-memory-web-api": "0.0.11",
        "bootstrap": "^3.3.6",
        "core-js": "^2.4.0",
        "jquery": "^2.2.4",
        "reflect-metadata": "^0.1.3",
        "rxjs": "5.0.0-beta.6",
        "semantic-ui": "^2.1.8",
        "systemjs": "0.19.27",
        "zone.js": "^0.6.12"
    },
    "devDependencies": {
        "concurrently": "^2.0.0",
        "css-loader": "^0.23.1",
        "extract-text-webpack-plugin": "^1.0.1",
        "file-loader": "^0.8.5",
        "find-remove": "^0.2.12",
        "html-loader": "^0.4.3",
        "html-webpack-plugin": "^2.15.0",
        "lite-server": "^2.2.0",
        "null-loader": "^0.1.1",
        "raw-loader": "^0.5.1",
        "rimraf": "^2.5.2",
        "semantic-ui": "^2.1.8",
        "style-loader": "^0.13.1",
        "ts-loader": "^0.8.1",
        "typescript": "^1.8.10",
        "typings": "^1.0.4",
        "webpack": "^1.13.0",
        "webpack-dev-server": "^1.14.1",
        "webpack-merge": "^0.14.0"
    }
}